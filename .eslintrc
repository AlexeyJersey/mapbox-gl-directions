{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "mapboxgl": true
  },
  "rules": {
    "strict": [0],
    "curly": [0],
    "no-alert": [0],
    "no-extra-bind": [0],
    "no-redeclare": [1],
    "new-cap": [0],
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "camelcase": [0],
    "consistent-return": [0],
    "no-underscore-dangle": [0],
    "comma-spacing": [0],
    "key-spacing": [0],
    "no-use-before-define": [0],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "react/no-multi-comp": 2,
    "react/prop-types": 2
  }
}
